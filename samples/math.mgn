// exercising math functions

{
    println: println
} := import('std')

{
    sum: sum
    min: min
    max: max
} := import('math')

{
    sort: sort
} := import('sort')

nums := [1, 2, 6, 35, -24, 100, -2]

println('sum:', sum(nums))
println('min:', min(nums))
println('max:', max(nums))

// sorting
println('sorted:', sort(nums))

