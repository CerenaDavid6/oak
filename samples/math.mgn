// exercising math functions

{
	println: println
} := import('std')

{
	printf: printf
} := import('fmt')

{
	sum: sum
	min: min
	max: max
} := import('math')

{
	sort: sort
} := import('sort')

nums := [1, 2, 6, 35, -24, 100, -2]

printf('sum: {{ 0 }}', sum(nums...))
printf('min: {{ 0 }}', min(nums...))
printf('max: {{ 0 }}', max(nums...))

// sorting
printf('sorted: {{ 0 }}', sort(nums))

